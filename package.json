{
  "name": "Ephemeres",
  "private": "true",
  "version": "0.0.0",
  "scripts": {
    "dev": "esno server",
    "build": "run-p build:client build:server",
    "build:static": "run-p build:client:static build:server",
    "build:client": "cross-env vite build --outDir ./dist/client --ssrManifest",
    "build:client:static": "cross-env vite build --outDir ./dist/static --ssrManifest",
    "build:server": "cross-env EPHEMERES_SERVER=true vite build --outDir ./dist/server --ssr src/entry-server.ts",
    "prerender": "esno server/prerender",
    "generate": "run-s build:static prerender",
    "serve": "cross-env NODE_ENV=production esno server",
    "serve:static": "serve ./dist/static"
  },
  "dependencies": {
    "naive-ui": "^2.24.1",
    "pinia": "^2.0.9",
    "pinia-store-decorators": "^1.1.1",
    "vue": "^3.2.27",
    "vue-router": "^4.0.12",
    "xicons": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/p": "^0.1.0",
    "@css-render/vue3-ssr": "^0.15.8",
    "@types/compression": "^1.7.2",
    "@types/html-minifier": "^4.0.2",
    "@types/node": "^17.0.9",
    "@types/prettier": "^2.4.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/server-renderer": "^3.2.27",
    "@vueuse/head": "^0.7.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "esno": "^0.13.0",
    "express": "^4.17.2",
    "fast-glob": "^3.2.11",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "serve": "^13.0.2",
    "serve-static": "^1.14.2",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.13",
    "vite": "^2.7.12",
    "vite-plugin-pages": "^0.19.9",
    "vite-plugin-vue-layouts": "^0.5.0"
  }
}